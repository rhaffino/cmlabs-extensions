{
  "manifest_version": 3,
  "name": "Ping Checker - cmlabs SEO Tools",
  "description": "Ping Tool helps you test the existence of a website's IP address/domain and analyzes its quality and responsiveness.",
  "version": "1.0.1",
  "action": {
    "default_popup": "views/index/index.html"
  },
  "icons": {
    "16": "assets/images/icon-ping-test.png",
    "32": "assets/images/icon-ping-test.png",
    "48": "assets/images/icon-ping-test.png",
    "128": "assets/images/icon-ping-test.png"
  },
  "background": {
    "service_worker": "service_worker/background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs"]
}
