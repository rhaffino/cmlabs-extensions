{
  "manifest_version": 3,
  "name": "Robots Checker",
  "description": "cmlabs tools for robots checker",
  "version": "1.0.0",
  "action": {
    "default_popup": "views/index/index.html"
  },
  "icons": {
    "16": "assets/images/website-16.png"
  },
  "background": {
    "service_worker": "service_worker/background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs"]
}
