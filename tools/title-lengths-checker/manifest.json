{
  "manifest_version": 3,
  "name": "Title & Meta Description Checkers - cmlabs SEO Tools",
  "description": "This tool helps you produce concise, attractive, and SEO-friendly elements to be seen on the SERP and attract readers.",
  "version": "1.0.1",
  "action": {
    "default_popup": "views/index/index.html"
  },
  "icons": {
    "16": "assets/images/icon-title-lengths-checker.png",
    "32": "assets/images/icon-title-lengths-checker.png",
    "48": "assets/images/icon-title-lengths-checker.png",
    "128": "assets/images/icon-title-lengths-checker.png"
  },
  "background": {
    "service_worker": "service_worker/background.js",
    "type": "module"
  },
  "permissions": ["storage", "tabs"]
}
